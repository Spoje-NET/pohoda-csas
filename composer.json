{
    "name": "vitexsoftware/pohoda-csas",
    "description": "A project integrating Pohoda with CSAS accounts API",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "vitezslav.dvorak@spojenet.cz"
        }
    ],
    "require": {
        "spojenet/csas-accountsapi": "^0.7.2",
        "vitexsoftware/pohoda-connector": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.87",
        "ergebnis/composer-normalize": "^2.48",
        "ergebnis/php-cs-fixer-config": "^6.55",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "autoload": {
        "psr-4": {
            "Pohoda\\Csas\\": "src/Pohoda/Csas/"
        }
    }
}
